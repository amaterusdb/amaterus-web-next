{
  "name": "amaterus-web-next",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "20.x",
    "npm": "10.x"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build && next-sitemap",
    "start": "next start",
    "lint": "next lint && prettier --check './**/*.{js,jsx,ts,tsx,json}'",
    "format": "next lint --fix && prettier --write './**/*.{js,jsx,ts,tsx,json}'",
    "generate": "graphql-codegen --require dotenv/config --config codegen.ts dotenv_config_path=.env.local"
  },
  "dependencies": {
    "@apollo/client": "3.11.4",
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@fontsource/roboto": "5.0.7",
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/introspection": "4.0.0",
    "@graphql-codegen/typescript": "4.0.1",
    "@graphql-codegen/typescript-operations": "4.0.1",
    "@graphql-codegen/typescript-react-apollo": "3.3.7",
    "@mui/icons-material": "5.14.3",
    "@mui/material": "5.14.3",
    "@types/node": "20.4.7",
    "@types/react": "18.2.18",
    "@types/react-dom": "18.2.7",
    "date-fns": "2.30.0",
    "eslint": "8.46.0",
    "eslint-config-next": "14.1.1",
    "eslint-config-prettier": "9.1.0",
    "graphql": "16.8.1",
    "next": "14.1.1",
    "next-sitemap": "4.2.3",
    "prettier": "3.2.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "5.1.6"
  }
}
